{
  "name": "marquez-website",
  "version": "1.0.0",
  "description": "My Personal Website",
  "author": "Julio Marquez",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "cross-env NODE_ENV=production nuxt generate --fail-on-error",
    "svg": "vsvg -s ./assets/svg/src -t ./components/icons --ext ts --es6",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "now-build": "yarn generate"
  },
  "dependencies": {
    "@nuxtjs/pwa": "3.0.0-beta.20",
