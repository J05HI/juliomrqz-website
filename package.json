{
  "name": "marquez-website",
  "version": "1.0.0",
  "description": "My Personal Website",
  "author": "Julio Marquez",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "start": "nuxt-ts start",
    "build": "cross-env NODE_ENV=production nuxt-ts build",
    "export": "cross-env NODE_ENV=production nuxt-ts export --fail-on-error",
    "generate": "yarn build && yarn export",
    "svg": "vsvg -s ./assets/svg/src -t ./components/icons --ext ts --es6",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "now-build": "yarn generate"
  },
  "dependencies": {
    "@nuxt/content": "1.0.0",
    "@nuxtjs/amp": "0.2.4",
    "@nuxtjs/pwa": "3.0.0-beta.20",
    "@nuxtjs/sentry": "4.0.3",
    "@nuxtjs/sitemap": "2.3.0",
    "@tailwindcss/ui": "0.3.0",
    "axios": "0.19.2",
    "date-fns": "2.14.0",
    "first-input-delay": "0.1.3",
    "github-markdown-css": "4.0.0",
    "intersection-observer": "0.10.0",
    "nuxt-edge": "2.13.0-26503223.f13b3752",
    "nuxt-i18n": "6.11.1",
    "nuxt-vitals": "1.0.7",
    "svgson": "4.1.0",
    "url-parse": "1.4.7",
    "validator": "13.0.0",
    "vue-lazy-hydration": "1.0.0-beta.12",
    "vue-lazyload": "1.3.3",
    "vue-scrollto": "2.18.1",
    "vue-svgicon": "3.2.6"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "1.0.1",
    "@ampproject/toolbox-optimizer": "2.4.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@nuxt/typescript-build": "1.0.0",
    "@nuxt/typescript-runtime": "0.4.7",
    "@nuxtjs/color-mode": "1.0.0",
    "@nuxtjs/eslint-config-typescript": "2.0.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/google-analytics": "2.3.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/tailwindcss": "2.0.0",
    "@types/cheerio": "0.22.18",
    "@types/fs-extra": "9.0.1",
    "@types/hash-sum": "1.0.0",
    "@types/url-parse": "1.4.3",
    "@types/validator": "13.0.0",
    "@vue/test-utils": "1.0.3",
    "amp-custom": "1.3.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "cheerio": "1.0.0-rc.3",
    "consola": "2.12.1",
    "cross-env": "7.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "fs-extra": "9.0.0",
    "hash-sum": "2.0.0",
    "husky": "4.2.5",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "8.0.0",
    "imagemin-svgo": "7.1.0",
    "jest": "26.0.1",
    "lint-staged": "10.2.6",
    "postcss-combine-duplicated-selectors": "8.1.0",
    "postcss-nested": "4.2.1",
    "prettier": "2.0.5",
    "reading-time": "1.2.0",
    "responsive-loader": "1.2.0",
    "sharp": "0.25.3",
    "stylelint": "13.5.0",
    "tailwindcss-dark-mode": "1.1.4",
    "tailwindcss-filters": "3.0.0",
    "ts-jest": "26.0.0",
    "vue-jest": "4.0.0-beta.2"
  },
  "engines": {
    "node": ">=12"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
