    "@tailwindcss/ui": "0.3.0",
    "date-fns": "2.13.0",
    "first-input-delay": "0.1.3",
    "github-markdown-css": "4.0.0",
    "intersection-observer": "0.10.0",
    "nuxt": "2.12.2",
    "nuxt-i18n": "6.11.0",
    "url-parse": "1.4.7",
    "validator": "13.0.0",
    "vue-lazy-hydration": "1.0.0-beta.12",
    "vue-lazyload": "1.3.3",
    "vue-script2": "2.1.0",
    "vue-scrollto": "2.18.1",
    "vue-svgicon": "3.2.6"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "1.0.0",
    "@commitlint/cli": "8.3.5",
