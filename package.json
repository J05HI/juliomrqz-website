    "imagemin-svgo": "7.1.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.0.0",
    "markdown-it": "10.0.0",
    "markdown-it-attrs": "3.0.2",
    "markdown-it-github-headings": "2.0.0",
    "markdown-it-inline-comments": "1.0.1",
    "markdown-it-link-attributes": "3.0.0",
    "markdown-it-mathjax": "2.0.0",
    "markdown-it-mathjax-node": "0.4.1",
    "markdown-it-prism": "2.0.5",
    "postcss-combine-duplicated-selectors": "8.1.0",
    "postcss-nested": "4.2.1",
    "prettier": "^2.0.5",
    "responsive-loader": "1.2.0",
    "sharp": "0.25.2",
    "stylelint": "^13.3.3",
    "tailwindcss-filters": "3.0.0",
    "ts-jest": "^25.0.0",
    "vue-jest": "^4.0.0-0"
  },
  "engines": {
    "node": ">=12"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
